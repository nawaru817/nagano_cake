<h1>商品一覧</h1>

<table>
    <thead>
      <tr>
        <th>商品ID</th>
        <th>商品名</th>
        <th>税抜価格</th>
        <th>ジャンル</th>
        <th>ステータス</th>
        <th colspan="5"></th>
      </tr>
    </thead>
    <tbody>
      <% @items = Item.all %>
      <% @items.each do |item| %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.name %></td>
        <td><%= item.price %></td>
        <td><%= item.genre_id %></td>

      </tr>
      <% end %>
    </tbody>
  </table>